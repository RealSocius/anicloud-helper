{
  "manifest_version": 2,
  "name": "Anicloud automator",
  "version": "1.0",
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["http://*/", "https://*/*"],
      "js": [
        "node_modules/file-saver/src/FileSaver.js",
        "jquery.js",
        "background.js"
      ]
    }
  ],
  "options_page": "options.html"
}
